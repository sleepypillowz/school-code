CREATE TABLE TBL_FAVANIME (
	FAVANIME VARCHAR PRIMARY KEY,
	KAGUYA_SAMA_LOVE_IS_WAR VARCHAR FOREIGN KEY REFERENCES TBL_AUTHOR(AKA_AKASAKA),
	FRUITS_BASKET VARCHAR,
    LINK_CLICK VARCHAR,
	STEINS_GATE VARCHAR,
	FULLMETAL_ALCHEMIST_BROTHERHOOD VARCHAR
);

CREATE TABLE TBL_AUTHOR (
	AUTHOR VARCHAR PRIMARY KEY,
    AKA_AKASAKA VARCHAR PRIMARY KEY,
    NATSUKI_TAKAYA VARCHAR,
    LI_HAOLING VARCHAR,
	YOMI_SARACHI VARCHAR,
	HIROMU_ARAKAWA VARCHAR 


CREATE TABLE TBL_GENRE (
	GENRE VARCHAR PRIMARY KEY,
    COMEDY VARCHAR ,
	ROMANCE VARCHAR,
    DRAMA VARCHAR,
	THRILLER VARCHAR,
	SHOUNEN VARCHAR
	
);


